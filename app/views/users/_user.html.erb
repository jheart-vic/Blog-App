<div>
    <div>
      <div>
        <img src="<%= user.photo %>" width="100" height="100" alt="Profile picture">
      </div>
      <div>
        <div>
         <h1>
          <%= user.name %>
          </h1>
          <div>
            <%= render layout: "shared/tag" do %>
              <p> Number of posts: <%= user.posts_counter %> </p>
            <% end %>
         </div>
        </div>
      </div>
    </div>
</div>
